/* Define the memory space of 4G bytes named "mem" */
define memory mem with size = 4G;

/*- Define region in memory -*/
define region IFLASH_region    = mem:[from __ICFEDIT_region_IFLASH_start__ to __ICFEDIT_region_IFLASH_end__] - mem:[from __FlashConfig1_start__ to __FlashConfig1_end__] - mem:[from __FlashConfig2_start__ to __FlashConfig2_end__] - mem:[from __FlashConfig3_start__ to __FlashConfig3_end__] - mem:[from __FlashConfig4_start__ to __FlashConfig4_end__] - mem:[from __FlashConfig5_start__ to __FlashConfig5_end__] - mem:[from __FlashConfig6_start__ to __FlashConfig6_end__] - mem:[from __FlashConfig7_start__ to __FlashConfig7_end__];
define region DFLASH_region    = mem:[from __ICFEDIT_region_DFLASH_start__ to __ICFEDIT_region_DFLASH_end__] - mem:[from __FlashConfig8_start__ to __FlashConfig8_end__];
define region RAM_region       = mem:[from __ICFEDIT_region_RAM_start__ to __ICFEDIT_region_RAM_end__];

define region FlashConfig1_region = mem:[from __FlashConfig1_start__ to __FlashConfig1_end__];
define region FlashConfig2_region = mem:[from __FlashConfig2_start__ to __FlashConfig2_end__];
