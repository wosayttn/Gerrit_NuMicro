/*- Define symbols -*/
define symbol __FlashConfig1_start__ = 0x0001FFC0;
define symbol __FlashConfig1_end__   = 0x0001FFC0;
define symbol __FlashConfig2_start__ = 0x0001FFC2;
define symbol __FlashConfig2_end__   = 0x0001FFC2;
